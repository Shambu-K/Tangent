#program to check if a number is a prime number

driver<: :>
(:
    var int n := 500;
    print(" Prime Numbers under 500:- \n");
    for(var int i = 1; i < n; i:= i + 1)
    (:
        var int count := 0;
        for(var int j := 1; j < i; j := j + 1)
        (:
            if(i%j = 0) 
            (:
                count:= count + 1;
            :)
        :)
        if(count = 2)
        (:
            print(i);
            print("\n");
        :)  
    :)
:)