#using jump statements

bool isEven<: var int a :>
(:
    var bool ans;
    if(a%2 = 0)
    (:
        ans = 1;
    :)
    else
    (:
        ans = 0;
    :)
    send ans;
:)

drive<: :>
(:
    var int a := 100;
    for(var int i := 1; i <= a; i := i + 1)
    (:
        if(i%10 = 0)
        (:
            continue;
        :) 
        var bool ans = isEven(i);
        if(ans)
        (:
            print(i);
        :)
        if(i%11 = 0) 
        (:
            break;
        :)
    :)
:)