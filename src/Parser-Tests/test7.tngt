#using relational operators

driver<: :>
(:
    var int a1 := 12;
    var int b1 := 20;
    var int c1 := 20;
    var int d1 := 10;

    if(a1 < b1)
    (:
        print(a1);
        print(" is less than ");
        print(b1);
    :)

    if(a1 = c1)
    (:
        print(a1);
        print(" is equal to ");
        print(c1);
    :)

    if(a1 > d1)
    (:
        print(a1);
        print(" is greater than ");
        print(d1);
    :)

:)