#using relational operators

driver<: :>
(:
    var int a := 12;
    var int b := 20;
    var int c := 20;
    var int d := 10;

    if(a < b)
    (:
        print(a);
        print(" is less than ");
        print(b);
    :)

    if(a = c)
    (:
        print(a);
        print(" is equal to ");
        print(c);
    :)

    if(a > d)
    (:
        print(a);
        print(" is greater than ");
        print(d);
    :)

:)