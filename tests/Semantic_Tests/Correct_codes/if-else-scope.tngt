#semantic and symbol table test for if-else scope

var int val := 2;
var bool check := false;

driver<: :>
(:
    var int a := 2, b := 4;
    var int ans := 0;
    if(check = true)
    (:
        var int a1 := a+2;
        var int b1 := b-2;

        if(val > 10)
        (:
            var int a1_1 := a*2;
            var int a1_2 := a*2;
            var int b1_1 := b*2;
            var int b1_2 := b*2;

            ans += a1_1-a1_2+b1_1-b1_2;
        :)

        else
        (:
            var int a1_3 := a/2;
            var int a1_4 := a/2;
            var int b1_3 := b/2;
            var int b1_4 := b/2;

            ans := a1_3+a1_4+b1_3+b1_4;
        :)
    :)

    else
    (:
        var int a2 := a+10;
        var int b2 := b/6;
    :)

    print(ans);

    
:)